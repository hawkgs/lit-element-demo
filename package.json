{
  "name": "lit-element-demo",
  "version": "1.0.0",
  "description": "lit-element demo application",
  "main": "index.js",
  "scripts": {
    "test": "karma start --coverage",
    "start": "polymer serve",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start --update-snapshots",
    "test:prune-snapshots": "karma start --prune-snapshots",
    "test:compatibility": "karma start --compatibility all --coverage",
    "test:compatibility:watch": "karma start --compatibility all --auto-watch=true --single-run=false",
    "test:bs": "karma start karma.bs.config.js --compatibility all --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hawkgs/lit-element-demo.git"
  },
  "author": "hawkgs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hawkgs/lit-element-demo/issues"
  },
  "homepage": "https://github.com/hawkgs/lit-element-demo#readme",
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.3.0",
    "lit-element": "^2.2.1"
  },
  "devDependencies": {
    "@open-wc/testing": "^2.0.0",
    "polymer-cli": "^1.9.11",
    "@open-wc/testing-karma": "^4.0.9",
    "webpack-merge": "^4.1.5",
    "@open-wc/testing-karma-bs": "^1.3.94"
  }
}
